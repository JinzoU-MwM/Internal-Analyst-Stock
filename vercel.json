{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://internal-analyst-frontend.vercel.app",
                "Access-Control-Allow-Methods": "GET,POST,PUT,PATCH,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type,Authorization",
                "Access-Control-Allow-Credentials": "true"
            },
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}